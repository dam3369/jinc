{
  "name": "jinc",
  "version": "1.0.0",
  "description": "friend finder",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.test.js",
    "watch-css": "stylus --watch --compress stylus/app.stylus --out dist/css/app.min.css",
    "watch-js": "watchify src/app.js -o dist/js/app.js",
    "watch": "npm run watch-js & npm run watch-css",
    "build": "browserify src/app.js -o dist/js/app.js | uglifyjs dist/js/app.js > dist/js/app.min.js"
  },
  "keywords": [
    "location",
    "friend"
  ],
  "author": "nicolas dufreche <n.dufreche@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "stylus": "^0.51.1",
    "tape": "^4.0.0",
    "uglify-js": "^2.4.23",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "es6-promise": "^2.1.1"
  }
}
